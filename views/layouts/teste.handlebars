  <script>
    $(window).scroll(function () {
      var height = $(window).scrollTop();
      if (height > 100) {
        $('.btn-scroll').fadeIn();
      } else {
        $('.btn-scroll').fadeOut();
      }
    });
    $(document).ready(function () {
      $(".btn-scroll").click(function (event) {
        event.preventDefault();
        $("html, body").animate({ scrollTop: 0 }, "slow");
        return false;
      });

    });

  </script>



